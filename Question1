a = [1, 2, 3, 4, 5, 6]        	           #given array
number = int(input())                      #given number to check wheather occurs in array

def CheckOccurance(arr, num):	
	for i in range(0, len(a)):	           #itterating through the array
		if a[i] == num:			           #checks wheather each iterating element is equal to the given number
			ans = "ocuur"
			break				           #breaks if find the number
		else: ans = "does not occur"
	return print(ans)

CheckOccurance(a, number)
